/[if !IE]>
%html{lang: 'en', class: 'no-js', ng:{ app: 'callCenter' }}
  /<![endif]
  %head
    %title "eLocal | Call Center"
    %meta(http-equiv="content-type" content="text/html" charset="utf-8")
    %meta(http-equiv="x-ua-compatible" content="ie=edge,chrome=1")
    %meta(name="description" content="")
    %meta(name="author" content="")

    -# Standard viewport tag to set the viewport to the device's width,
    -# Android 2.3 devices need this so 100% width works properly and
    -# doesn't allow children to blow up the viewport width
    %meta{content: "initial-scale=1.0,user-scalable=no,maximum-scale=1,width=device-width", name: "viewport"}

    -# Fix for iPhone 5 fullscreen web apps
    %meta(name="viewport" content="initial-scale=1.0,user-scalable=no,maximum-scale=1" media="(device-height: 568px)")
    %meta(name='apple-mobile-web-app-capable' content='yes')
    %meta(name='apple-mobile-web-app-status-bar-style' content='translucent-black')

    -# App Icons
    %link(rel="shortcut icon" href="/images/favicon.ico")
    %link(rel="apple-touch-icon-precomposed" href="/images/apple-touch-icon-precomposed.png")
    %link(rel="apple-touch-icon-precomposed" sizes="57x57" href="/images/apple-touch-icon-57x57-precomposed.png")
    %link(rel="apple-touch-icon-precomposed" sizes="72x72" href="/images/apple-touch-icon-72x72-precomposed.png")
    %link(rel="apple-touch-icon-precomposed" sizes="114x114" href="/images/apple-touch-icon-114x114-precomposed.png")

    -# Fonts
    %link{href: "https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css", rel: "stylesheet", integrity: "sha256-3dkvEK0WLHRJ7/Csr0BZjAWxERc5WH7bdeUya2aXxdU= sha512-+L4yy6FRcDGbXJ9mPG8MT/3UCDzwR9gPeyFNMCtInsol++5m3bk2bXWKdZjvybmohrAsn3Ua5x8gfLnbE1YkOg==", crossorigin: "anonymous"}

    -# Stylesheets
    %link{href: "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css", rel: "stylesheet", integrity: "sha256-7s5uDGW3AHqw6xtJmNNtr+OBRJUlgkNJEo78P4b0yRw= sha512-nNo+yCHEyn0smMxSswnf/OnX6/KwJuZTlNZBjauKhTK0c+zT+q5JOCx0UFhXQ6rJR9jg6Es8gPuD2uZcYDLqSw==", crossorigin: "anonymous"}
    = stylesheet_link_tag 'application'

  %body#body{class: current_page.data.body_class || 'page', ng:{controller:'NotificationController as notifications'}}
    = javascript_include_tag '//ajax.googleapis.com/ajax/libs/angularjs/1.3.14/angular.min.js'
    %nav.navbar.navbar-default.navbar-fixed-top{ role: 'navigation' }
      .container
        .navbar-header
          %button.navbar-toggle{ data:{target: '#main-top-navbar-collapse',toggle: 'collapse'}, type:'button'}
            %span.sr-only Toggle navigation
            %span.icon-bar
            %span.icon-bar
            %span.icon-bar
          %a.navbar-brand.visible-desktop{href: '/'}
            eLocal
            %span.small .com


        .collapse.navbar-collapse#main-top-navbar-collapse
          %ul.nav.navbar-nav.navbar-right
            %li
              %a{ href: '#'}
                %i.fa.fa-futbol-o
                Manage Hangup Dispositions
            %li
              %a{ href: '#'}
                %i.fa.fa-users
                Manage Users
            %li
              %a{ href: '#'}
                %i.fa.fa-file-text
                Manage Call Scripts


            %li
              %a{ rel:"nofollow", href: '#', data:{method: "delete"} }
                %i.fa.fa-sign-out
                &nbsp;Sign Out


    .sub-nav-container
      .container
        .sub-nav-title
          %h1 Call Center
        %ul.sub-nav-list.list-inline
          %li Current User: development@elocal.com
          %li Your Phone Number: 610-629-6429
          %li
            %a.btn.btn-light-blue{ href: '#' } Edit Your Information

    .wrapper
      = yield

    #id_1
    .footer
      .container
        .row
          .col-xs-12
            %p.copyright.text-center &copy;2018 eLocal USA LLC

    -# Javascripts
    = javascript_include_tag '//cdnjs.cloudflare.com/ajax/libs/modernizr/2.6.2/modernizr.min.js'
    = javascript_include_tag '//cdnjs.cloudflare.com/ajax/libs/jquery/2.0.3/jquery.min.js'
    %script{src: "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js", integrity: "sha256-KXn5puMvxCw+dAYznun+drMdG1IFl3agK0p/pqT9KAo= sha512-2e8qq0ETcfWRI4HJBzQiA3UoyFk6tbNyG+qSaIBZLyW9Xf3sWZHN/lxe9fTh1U45DpPf07yj94KsUHHWe4Yk1A==", crossorigin: "anonymous"}
    = javascript_include_tag 'application'